{
  "name": "lninl-ui-example",
  "version": "0.0.1",
  "description": "A uni-app UI components library",
  "author": "lninl-ui",
  "license": "MIT",
  "homepage": "https://github.com/lninl-com/lninl-ui#readme",
  "repository": "git+https://github.com/lninl-com/lninl-ui",
  "bugs": {
    "url": "https://github.com/lninl-com/lninl-ui/issues"
  },
  "keywords": [
    "lninl-ui",
    "TDesign",
    "unocss",
    "i18n",
    "dark-mode",
    "lninl",
    "UI",
    "uniapp",
    "uni-app",
    "components",
    "vue",
    "vue3"
  ],
  "scripts": {
    "prepare": "npx simple-git-hooks",
    "dev:app": "uni -p app",
    "dev:app-android": "uni -p app-android",
    "dev:app-ios": "uni -p app-ios",
    "dev:custom": "uni -p",
    "dev:h5": "uni",
    "dev:h5-test": "uni --auto-port 9520",
    "dev:h5:ssr": "uni --ssr",
    "dev:mp-alipay": "uni -p mp-alipay",
    "dev:mp-baidu": "uni -p mp-baidu",
    "dev:mp-jd": "uni -p mp-jd",
    "dev:mp-kuaishou": "uni -p mp-kuaishou",
    "dev:mp-lark": "uni -p mp-lark",
    "dev:mp-qq": "uni -p mp-qq",
    "dev:mp-toutiao": "uni -p mp-toutiao",
    "dev:mp-weixin": "uni -p mp-weixin",
    "dev:mp-xhs": "uni -p mp-xhs",
    "dev:quickapp-webview": "uni -p quickapp-webview",
    "dev:quickapp-webview-huawei": "uni -p quickapp-webview-huawei",
    "dev:quickapp-webview-union": "uni -p quickapp-webview-union",
    "build:app": "uni build -p app",
    "build:app-android": "uni build -p app-android",
    "build:app-ios": "uni build -p app-ios",
    "build:custom": "uni build -p",
    "build:h5": "uni build",
    "build:h5:ssr": "uni build --ssr",
    "build:mp-alipay": "uni build -p mp-alipay",
    "build:mp-baidu": "uni build -p mp-baidu",
    "build:mp-jd": "uni build -p mp-jd",
    "build:mp-kuaishou": "uni build -p mp-kuaishou",
    "build:mp-lark": "uni build -p mp-lark",
    "build:mp-qq": "uni build -p mp-qq",
    "build:mp-toutiao": "uni build -p mp-toutiao",
    "build:mp-weixin": "uni build -p mp-weixin",
    "build:mp-xhs": "uni build -p mp-xhs",
    "build:quickapp-webview": "uni build -p quickapp-webview",
    "build:quickapp-webview-huawei": "uni build -p quickapp-webview-huawei",
    "build:quickapp-webview-union": "uni build -p quickapp-webview-union",
    "test:app": "cross-env UNI_PLATFORM=app jest -i",
    "test:app-android": "cross-env UNI_PLATFORM=app UNI_OS_NAME=android jest -i",
    "test:app-ios": "cross-env UNI_PLATFORM=app UNI_OS_NAME=ios jest -i",
    "test:h5": "cross-env NODE_ENV=development UNI_PLATFORM=h5 jest -i --detectOpenHandles",
    "test:mp-alipay": "cross-env UNI_PLATFORM=mp-alipay jest -i",
    "test:mp-baidu": "cross-env UNI_PLATFORM=mp-baidu jest -i",
    "test:mp-jd": "cross-env UNI_PLATFORM=mp-jd jest -i",
    "test:mp-kuaishou": "cross-env UNI_PLATFORM=mp-kuaishou jest -i",
    "test:mp-lark": "cross-env UNI_PLATFORM=mp-lark jest -i",
    "test:mp-qq": "cross-env UNI_PLATFORM=mp-qq jest -i",
    "test:mp-toutiao": "cross-env UNI_PLATFORM=mp-toutiao jest -i",
    "test:mp-weixin": "cross-env UNI_PLATFORM=mp-weixin jest -i",
    "test:mp-xhs": "cross-env UNI_PLATFORM=mp-xhs jest -i",
    "test:quickapp-webview": "cross-env UNI_PLATFORM=quickapp-webview jest -i",
    "test:quickapp-webview-huawei": "cross-env UNI_PLATFORM=quickapp-webview-huawei jest -i",
    "test:quickapp-webview-union": "cross-env UNI_PLATFORM=quickapp-webview-union jest -i",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@dcloudio/uni-app": "3.0.0-alpha-4010120240403003",
    "@dcloudio/uni-app-plus": "3.0.0-alpha-4010120240403003",
    "@dcloudio/uni-components": "3.0.0-alpha-4010120240403003",
    "@dcloudio/uni-h5": "3.0.0-alpha-4010120240403003",
    "@dcloudio/uni-mp-alipay": "3.0.0-alpha-4010120240403003",
    "@dcloudio/uni-mp-baidu": "3.0.0-alpha-4010120240403003",
    "@dcloudio/uni-mp-jd": "3.0.0-alpha-4010120240403003",
    "@dcloudio/uni-mp-kuaishou": "3.0.0-alpha-4010120240403003",
    "@dcloudio/uni-mp-lark": "3.0.0-alpha-4010120240403003",
    "@dcloudio/uni-mp-qq": "3.0.0-alpha-4010120240403003",
    "@dcloudio/uni-mp-toutiao": "3.0.0-alpha-4010120240403003",
    "@dcloudio/uni-mp-weixin": "3.0.0-alpha-4010120240403003",
    "@dcloudio/uni-mp-xhs": "3.0.0-alpha-4010120240403003",
    "@dcloudio/uni-quickapp-webview": "3.0.0-alpha-4010120240403003",
    "dayjs": "~1.11.10",
    "lninl-ui": "link:src/uni_modules/lninl-ui",
    "luch-request": "^3.1.1",
    "pinia": "~2.0.36",
    "pinia-plugin-persistedstate": "~3.2.1",
    "vue": "3.4.21",
    "vue-i18n": "^9.11.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.13.2",
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@dcloudio/types": "^3.4.8",
    "@dcloudio/uni-automator": "3.0.0-alpha-4010120240403003",
    "@dcloudio/uni-cli-shared": "3.0.0-alpha-4010120240403003",
    "@dcloudio/uni-stacktracey": "3.0.0-alpha-4010120240403003",
    "@dcloudio/vite-plugin-uni": "3.0.0-alpha-4010120240403003",
    "@playwright/test": "^1.43.0",
    "@uni-helper/vite-plugin-uni-pages": "^0.2.17",
    "@unocss/eslint-plugin": "~0.58.9",
    "@vue/runtime-core": "3.4.21",
    "adbkit": "^2.11.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "jest": "27.0.4",
    "jest-environment-node": "27.5.1",
    "jest-image-snapshot": "6.1.0",
    "lint-staged": "^15.2.2",
    "node-simctl": "^6.6.0",
    "playwright": "^1.43.0",
    "postcss-selector-parser": "^6.0.16",
    "puppeteer": "^21.11.0",
    "simple-git-hooks": "^2.11.1",
    "unocss": "~0.58.9",
    "unocss-preset-weapp": "^0.59.1",
    "vite": "^4.5.3"
  },
  "simple-git-hooks": {
    "pre-commit": "npx --no-install -- lint-staged",
    "commit-msg": "npx --no-install commitlint -c --edit"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
